# 初始gas设置
INITIAL_GAS_PRICE_GWEI="5"
INITIAL_GAS_LIMIT="300000"

# 并发控制
MAX_CONCURRENT_WALLETS="10"
MAX_RETRIES="3"

# RPC URL（BSC主网）
RPC_URL="https://bsc-dataseed.binance.org/"

# 执行模式（fast: 快速模式，ultra: 超高速模式）
EXECUTION_MODE="ultra"

# 参与抢购的钱包数量（1-50）
WALLET_COUNT="50"

# ========== 签名配置 ==========
# 是否需要签名（true/false）
SIGNATURE_REQUIRED="false"

# 签名类型（http/static/custom）
SIGNATURE_TYPE="http"

# HTTP签名API配置
SIGNATURE_API_URL="https://api.example.com/sign/{address}"
SIGNATURE_API_KEY=""
SIGNATURE_METHOD="POST"
SIGNATURE_HEADERS='{"Authorization": "Bearer YOUR_TOKEN"}'

# 静态签名（用于测试）
STATIC_SIGNATURE=""

# 签名位置（append/prepend/replace）
SIGNATURE_POSITION="append"